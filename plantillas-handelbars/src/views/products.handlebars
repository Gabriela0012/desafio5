<div class="container mb-3">
  <h1>PRODUCTOS</h1>
  <a href='/'>
    <button type='button'class="btn btn-info">INICIO</button>
  </a>
  
  <table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">producto</th>
      <th scope="col">precio</th>
      <th scope="col">imagen</th>
    </tr>
  </thead>
  {{#each products}}
  <tbody>
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td>
        
        <img class="img" height='50rem' src="http://localhost:8080/img/{{this.image}}"></td>
    </tr>
  
  </tbody>
  {{/each}}
</table>
  
  
</div>